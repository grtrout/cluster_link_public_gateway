link.mode=BIDIRECTIONAL
connection.mode=OUTBOUND
remote.link.connection.mode=INBOUND

security.protocol=SASL_SSL
sasl.mechanism=PLAIN
sasl.jaas.config=org.apache.kafka.common.security.plain.PlainLoginModule required username='[API_KEY]' password='[API_SECRET]';

local.security.protocol=SASL_SSL
local.sasl.mechanism=PLAIN
local.sasl.jaas.config=org.apache.kafka.common.security.plain.PlainLoginModule required username='[API_KEY]' password='[API_SECRET]';
